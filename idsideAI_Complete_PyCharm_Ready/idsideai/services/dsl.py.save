class DecisionModelSpec(BaseModel):
    name: str
    description: str = ""
    steps: List[Step]


def parse_sdl(text: str) -> DecisionModelSpec:
    """Parse SDL YAML into a DecisionModelSpec."""
    try:
        data = yaml.safe_load(text)
        return DecisionModelSpec(**data)
    except Exception as e:
        # Preserve original traceback/context for Ruff B904
        raise ValueError(f"SDL parse error: {e}") from e
